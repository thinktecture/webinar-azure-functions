@page "/"
@using Microsoft.AspNetCore.Components.Authorization


<h1>Hello, world!</h1>

Welcome to your new app.

<AuthorizeView>
    <dl>
        @foreach (var claim in context.User.Claims)
        {
            <dt>@claim.Type</dt>
            <dd>@claim.Value</dd>
        }
    </dl>
</AuthorizeView>
